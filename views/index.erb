
<h1>Artists</h1>
<h3>All of the artists in the Artist table:</h3>
<% @artists.each do |artist| %>
 			<a href=<%="/artist/"+artist.id.to_s%>><%= artist.fname + " " + artist.lname %></a>
 			<br>
<% end %> 

<h1>Artwork</h1>
<h3>All of the artworks in the Artwork table:</h3>
<% @artworks.each do |artwork| %>
 		<figure>
 			<a href=<%="/artwork/"+artwork.id.to_s%>><img src= <%= artwork.image_url %>></a>
  		<figcaption><%= artwork.title %></figcaption>
		</figure>
<% end %> 

